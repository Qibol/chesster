<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Runnable4Chesster" basedir=".">
    <target name="create_run_jar">
        <jar destfile="${basedir}/Chess.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="com.chesster.Executor"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="D:/Warehouse/workspace/Chesster/bin"/>
        </jar>
    </target>
</project>
